@model IEnumerable<symphony_limited.FrameWork.Result>



<div id="menuLogin" styler style="font-family: sans-serif;color: #333;">

    <ul class="" style="float:right !important;">

        @if (Session["Email"] != null)
        {
            <li style="margin-right:80px"><a style="color:red;margin-top:;margin-right:200px">  @Session["Email"]</a></li>
            <li>@Html.ActionLink("Logout", "Logout", "LoginRegister")</li>
            <li style="color:black">@Html.ActionLink("Create New", "Create")</li>
        }
        else
        {
            <li>@Html.ActionLink("Signup", "Signup", "LoginRegister")</li>
            <li>@Html.ActionLink("Login", "Login", "LoginRegister")</li>
            <li style="color:black">@Html.ActionLink("Create New", "Create")</li>
        }
    </ul>
</div>
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_layout.cshtml";
}

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Student.NameStudent)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NameReult)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Point)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Student.NameStudent)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NameReult)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Point)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
